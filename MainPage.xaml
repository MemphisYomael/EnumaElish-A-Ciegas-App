<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:EnumaElishApp"
             xmlns:components="clr-namespace:EnumaElishApp.Components"
             xmlns:paginas="clr-namespace:EnumaElishApp.Components.Pages"
             
             x:Class="EnumaElishApp.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type paginas:Home}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
